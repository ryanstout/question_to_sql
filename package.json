{
  "name": "nlpquery-6deb",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "remix build --sourcemap",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build --bundle",
    "dev": "run-p dev:*",
    "dev:build": "cross-env NODE_ENV=development npm run build:server -- --watch",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:server": "cross-env NODE_ENV=development node --inspect --require ./node_modules/dotenv/config --require ./mocks ./build/server.js",
    "docker": "docker-compose up -d",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "start:mocks": "cross-env NODE_ENV=production node --require ./mocks --require dotenv/config ./build/server.js",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
    "typecheck": "tsc -b && tsc -b cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "prettier": {
    "semi": false,
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "importOrderGroupNamespaceSpecifiers": true,
    "importOrder": [
      "^[:alpha].*$",
      "^@remix(.*)$",
      "^(@mantine|@emotion|@nivo)(.*)$",
      "^@prisma(.*)$",
      "^~/"
    ]
  },
  "eslintIgnore": [
    "/bin",
    "/prisma/seed.ts",
    "/node_modules",
    "/build",
    "/public/build",
    "/postgres-data"
  ],
  "dependencies": {
    "@codemirror/lang-sql": "^6.3.3",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@mantine/core": "^5.10.3",
    "@mantine/dates": "^5.10.0",
    "@mantine/form": "^5.10.0",
    "@mantine/hooks": "^5.10.3",
    "@mantine/modals": "^5.10.0",
    "@mantine/prism": "^5.10.0",
    "@mantine/remix": "^5.10.0",
    "@nivo/bar": "^0.80.0",
    "@nivo/core": "^0.80.0",
    "@nivo/line": "^0.80.0",
    "@prisma/client": "4.8.0",
    "@remix-run/express": "^1.10.0",
    "@remix-run/node": "^1.10.0",
    "@remix-run/react": "^1.10.0",
    "@remix-run/server-runtime": "^1.10.0",
    "@remix-validated-form/with-zod": "^2.0.5",
    "@sentry/remix": "^7.34.0",
    "@tabler/icons": "^1.119.0",
    "@uiw/react-codemirror": "^4.19.5",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.7",
    "express": "^4.18.2",
    "isbot": "^3.6.3",
    "json-beautify": "^1.1.1",
    "loglevel": "^1.8.1",
    "loglevel-plugin-prefix": "^0.8.4",
    "mantine-datatable": "^1.8.2",
    "morgan": "^1.10.0",
    "prisma": "^4.8.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-validated-form": "^4.6.7",
    "tiny-invariant": "^1.3.1",
    "zod": "^3.20.2",
    "zodix": "^0.4.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@remix-run/dev": "^1.9.0",
    "@remix-run/eslint-config": "^1.9.0",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cookie": "^0.5.1",
    "@types/express": "^4.17.14",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.9",
    "@types/prop-types": "^15.7.5",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@vitejs/plugin-react": "^2.2.0",
    "@vitest/coverage-c8": "^0.24.5",
    "autoprefixer": "^10.4.13",
    "c8": "^7.12.0",
    "cookie": "^0.5.0",
    "cypress": "^10.11.0",
    "dotenv": "^16.0.3",
    "esbuild": "^0.15.13",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "happy-dom": "^6.0.4",
    "msw": "^0.47.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "pyright": "^1.1.292",
    "start-server-and-test": "^1.14.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.8.4",
    "typesync": "^0.9.2",
    "vite": "^3.2.2",
    "vite-tsconfig-paths": "^3.5.2",
    "vitest": "^0.24.5"
  },
  "engines": {
    "node": "19.2.0",
    "npm": "9.4.2"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  },
  "typesync": {
    "ignorePackages": [
      "eslint",
      "prettier",
      "prettier-plugin-sort-imports"
    ]
  }
}
